{  
  "ServiceOptions": {
    "Name": "Gateway",
    "Version": "v1.0",
    "ServiceManagementEndpoint": "http://localhost:56456"
  },
  "ConfigurationServiceOptions": {
    "Endpoint": "http://localhost:56456/api/configurations",
    "RetryCount": 5,
    "DelayRetry": 1000
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
